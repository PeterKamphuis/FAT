pro colormaps,color_map,INVERT=invert

;+
; NAME:
;       COLORMAPS
;
; PURPOSE:
;       Collection of many different colormaps used throughout
;       astronomy for easy use in IDL.
;
; CATEGORY:
;       Support
; 
; CALLING SEQUENCE:
;       COLORMAPS,color_map,EXAMPLE=example,INVERT=invert
;
;
; INPUTS:
;     color_map = string containing the name of the requested colormap
;
; OPTIONAL INPUTS:
;       - 
;
; KEYWORD PARAMETERS:
;       /INVERT - invert the requested colormap
;
; OUTPUTS:
;       The requested colormap is loaded into IDLs colorscheme
;
; OPTIONAL OUTPUTS:
;       -
;
; PROCEDURES CALLED:
;       
;
; EXAMPLE:
;      
;
; MODIFICATION HISTORY:
;       22-09-2009 P.Kamphuis; Added an error message  
;       Written 03-09-2009 by P.Kamphuis
;
; NOTE:
;     I only collected these maps and had no hand in developing
;     them. Most are common colormaps and are not accredited in
;     software. The sauron_colormap is by Michele Cappellari & Eric
;     Emsellem, Leiden, 10 July 2001 and I have made some variations.
;     The other colormaps are taken from the GIPSY distribution  (vd
;     Hulst et al.1992, https://www.astro.rug.nl/~gipsy/) 
;-

  CATCH,Error_status
  IF  Error_status NE 0. THEN BEGIN
     print, 'Oops the following went wrong:'
     print, !ERROR_STATE.MSG
     print,'Use colormaps in this way:'
     print, "CALLING SEQUENCE: colormaps,'color_map',/INVERT"
     print,  '/INVERT inverts the chosen colormap'
     print, 'Colormap options are:sauron_colormap,white_sauron_colormap,'
     print, 'black_sauron_colormap,experimental_colormap,ronekers,'
     print, 'aips,alex,blue,blulut,bw,color,flag,green,halley,heat,ikon,'
     print, 'isophot,kester,light,lnspiral,manycol,mousse,pastel,'
     print, 'pseudo1,pseudo2,rainbow,rainbow1,rainbow2,rainbow3,rainbow4,'
     print, 'rainbow5,ramp,random,random1,random2,random3,random4,real,red,'
     print, 'smooth,staircase,stairs8,stairs9,standard,hot'
     goto,ending
  endif
  color_map=STRLOWCASE(color_map)
  case color_map of
     'sauron_colormap': begin
;sauron_colormap
; Michele Cappellari & Eric Emsellem, Leiden, 10 July 2001
